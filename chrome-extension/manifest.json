{
  "manifest_version": 2,

  "name": "funnel.ai",
  "description": "Extension of funnel.ai",
  "version": "0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://outlook.office.com/*"]
  }],

  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
